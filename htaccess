RewriteEngine On

RewriteRule ^feed/ rss.php [L]
RewriteRule ^feed/rss rss.php [L]
RewriteRule ^RSS rss.php [L]
RewriteRule ^rss rss.php [L]
RewriteRule ^rss.xml rss.php [L]
ReWriteRule ^itiraf-([0-9]+).html index.php?ney=itiraf&id=$1 [L,NC]

ReWriteRule ^m-msj.html m-msj.php [L,NC]
ReWriteRule ^m-msj-([0-9]+).html m-msj.php?gnid=$1&msj=oku [L,NC]
ReWriteRule ^f-mesaj-([0-9]+).html f-mesaj.php?gnid=$1&msj=oku [L,NC]

